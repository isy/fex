{
  "name": "fex",
  "description": "Nuxt.js TypeScript project",
  "version": "2.6.1",
  "private": true,
  "dependencies": {
    "@nuxtjs/axios": "5.4.1",
    "nuxt": "2.6.1",
    "nuxt-property-decorator": "2.1.3",
    "ts-node": "8.0.3",
    "vuex-class": "0.3.2"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:fix": "eslint --ext .js,.vue,.ts --fix --ignore-path .gitignore .",
    "precommit": "lint-staged",
    "test:unit": "jest --coverage --maxWorkers=5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@nuxt/typescript": "2.6.1",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/node": "10.14.4",
    "@types/webpack-env": "1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-pug": "^1.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0"
  }
}
